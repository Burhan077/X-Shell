setopt localoptions NO_shwordsplit
local cdvbase

cdvbase=${vcs_comm[basedir]}
rrn=${cdvbase:t}
VCS_INFO_formats '' "${cdvbase:t}" "${cdvbase}" '' '' '' ''
return 0
